{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0",
  "background": {
     "page": "background.html"
      , "persistent": true
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "img/upwork.png"
  },
  "permissions": [
    "storage" ,
    "contextMenus",
    "tabs",
    "notifications",
    "https://www.upwork.com/",
    "https://ajax.googleapis.com/"
  ],
  "version": "1.0.5"
}
